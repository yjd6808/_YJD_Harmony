<ResourceDictionary  x:Class="SGToolsUI.ModelTemplate.UIButtonCanvasTemplate"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mo_main="clr-namespace:SGToolsUI.Model.Main">
    <DataTemplate x:Key="ButtonCanvasTemplate" DataType="{x:Type mo_main:SGUIButton}">
        <Image Name="ButtonImage" Source="{Binding VisualSpriteSource}"/>
    </DataTemplate>
</ResourceDictionary>