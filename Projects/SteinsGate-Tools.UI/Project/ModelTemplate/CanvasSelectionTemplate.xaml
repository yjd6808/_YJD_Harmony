<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:local="clr-namespace:SGToolsUI"
                    xmlns:mo="clr-namespace:SGToolsUI.Model"
                    xmlns:vm="clr-namespace:SGToolsUI.ViewModel">
    <DataTemplate x:Key="CanvasSelectionTemplate"
                  DataType="{x:Type mo:CanvasSelection}">
        <ContentPresenter Margin="{Binding Element.VisualRect, Converter={x:Static local:Constant.RectThicknessConverter}}" Content="{Binding Selection}">

        </ContentPresenter>
    </DataTemplate>
    
</ResourceDictionary>