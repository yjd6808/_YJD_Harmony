<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mo_backup="clr-namespace:SGToolsUI.Model.Backup">
    <HierarchicalDataTemplate x:Key="BackupTreeViewItemGroupTemplate"
                              DataType="{x:Type mo_backup:BackupTreeViewItemGroup}"
                              ItemsSource="{Binding Children}">
        <StackPanel Orientation="Horizontal">
            <Image Width="16" Height="16" Margin="0, 0, 4, 0" Source="{Binding Path=UIElementType, Converter={StaticResource UIElementTypeColoredImageConverter}}"/>
            <TextBlock Name="VisualNameTextBlock" Text="{Binding VisualName}" HorizontalAlignment="Stretch" />
        </StackPanel>
    </HierarchicalDataTemplate>
</ResourceDictionary>