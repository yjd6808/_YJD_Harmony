<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>SGToolsCommon</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <BaseIntermediateOutputPath>..\Intermediate\</BaseIntermediateOutputPath>
    <BaseOutputPath>..\Out\</BaseOutputPath>
    <RunPostBuildEvent>Always</RunPostBuildEvent>
    <AssemblyName>SGToolsCommon</AssemblyName>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="rd &quot;$(ProjectDir)obj&quot; /S /Q" />
  </Target>

  <ItemGroup>
    <PackageReference Include="Syncfusion.PropertyGrid.WPF" Version="20.4.0.51" />
    <PackageReference Include="System.Drawing.Common" Version="7.0.0" />
    <PackageReference Include="System.Reflection" Version="4.3.0" />
  </ItemGroup>

</Project>
